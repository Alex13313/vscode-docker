version: '3.4'

services:
  {{ serviceName }}:
    image: {{ serviceName }}
    build:
      context: .
      dockerfile: Dockerfile
    command: {{ toQuotedArray pythonDebugCmdParts }}
{{#if ports}}
    ports:
{{#each ports}}
      - {{ . }}:{{ . }}
{{/each}}
{{/if}}
{{#if (eq platform 'Python: Flask')}}
    environment:
      - FLASK_APP={{#if pythonArtifact.file}}{{ pythonArtifact.file }}{{else}}-m {{ pythonArtifact.module }}{{/if}}
{{/if}}
